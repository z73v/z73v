function plugin_LOST(msg)
for v in io.popen('ls File_Bot'):lines() do
if v:match(".lua$") then
plugin = dofile("File_Bot/"..v)
if plugin.LOST and msg then
pre_msg = plugin.LOST(msg)
end
end
end
send(msg.chat_id_, msg.id_,pre_msg)  
end

