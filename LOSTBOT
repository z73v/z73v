tdcli_function ({ID = "GetUser",user_id_ = result.sender_user_id_},function(arg,data) 
usertext = '\nâ„˜ : Ø§Ù„Ø¹Ø¶Ù€Ù€Ùˆ Â» ['..data.first_name_..'](t.me/'..(data.username_ or 'DEVBESSO')..')'
